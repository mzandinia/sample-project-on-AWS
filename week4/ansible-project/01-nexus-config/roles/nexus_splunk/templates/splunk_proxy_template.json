{
  "name": "{{ splunk_repo_name }}",
  "online": true,
  "storage": {
    "blobStoreName": "{{ raw_blob_store_name }}",
    "strictContentTypeValidation": true
  },
  "cleanup": {
    "policyNames": [
      "string"
    ]
  },
  "proxy": {
    "remoteUrl": "{{ splunk_remoteUrl }}",
    "contentMaxAge": 1440,
    "metadataMaxAge": 1440
  },
  "negativeCache": {
    "enabled": true,
    "timeToLive": 1440
  },
  "httpClient": {
    "blocked": false,
    "autoBlock": true
  },
  "routingRule": "string",
  "replication": {
    "preemptivePullEnabled": false,
    "assetPathRegex": "string"
  },
  "raw": {
    "contentDisposition": "ATTACHMENT"
  }
}
